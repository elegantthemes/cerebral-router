{
  "name": "urlon",
  "version": "3.0.1",
  "description": "URL Object Notation. JSON-like encoding for URLs",
  "scripts": {
    "pretest": "standard lib/*",
    "test": "jasmine",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "dist": "umd -c urlon < lib/urlon.js > dist/urlon.umd.js",
    "prepublish": "npm run test && npm run dist"
  },
  "keywords": [
    "urlon",
    "json",
    "notation",
    "url"
  ],
  "homepage": "https://github.com/cerebral/urlon",
  "author": {
    "name": "Vjeux",
    "email": "vjeuxx@gmail.com",
    "url": "http://blog.vjeux.com/"
  },
  "contributors": [
    "Aleksey Guryanov <gurianov@gmail.com>"
  ],
  "files": [
    "lib/urlon.js",
    "dist/urlon.umd.js"
  ],
  "main": "lib/urlon.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cerebral/urlon.git"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "jasmine": "^2.4.1",
    "nyc": "^11.3.0",
    "standard": "^10.0.3",
    "umd": "^3.0.1"
  },
  "nyc": {
    "exclude": [
      "spec"
    ]
  }
}
